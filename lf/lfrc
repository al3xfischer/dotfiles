map e $code $env:f -n
map E $code $env:f -r
map n $nvim $env:f
map gt $nvim \Users\Alex\todo.md
map gl cd ~/Downloads
map gi cd ~/itm
map gd cd ~/Desktop
map gr cd /
map gp cd ~/projects
map gwp cd /mnt/c/Users/Alex/projects/
map gwp cd /mnt/c/Users/Alex/projects/
map st :set sortby time; set info time; set reverse!
map <enter> open
map w $bash
map D delete $env:f
map u mkdir

